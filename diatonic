<script type="text/javascript">

  $(".hover-chord").hover(function(){
    var chordType = $(this).attr("attr-type");
    var offset = $(this).attr("attr-offset");
    $("#count2").addClass(chordType);
    $("#count2").css("margin-left", (5+(offset*41))+"px");
  }, function(){
    $("#count2").removeClass("major minor diminished");
  });

  $(".key").click(function(){
    $(".key").removeClass("selected");
    $(this).addClass("selected");
    var key = $(this).html();
    var offset = parseInt($(this).attr("attr-note-count"));
    $("#key-offset").html(offset);
    $("#tool-key").html(key);
    $(".note-tool .note").removeClass("inactive").addClass("active");
    $(".note-tool-container").animate({right:  offset*41 + "px"}, 400);

    $(".hover-chord").each(function(){
      var noteOffset = parseInt($(this).attr("attr-offset"));
      var globalOffset = parseInt($("#key-offset").html());
      var notes = ["A", "A#", "B", "C", "C#", "D", "D#", "E", "F", "F#", "G", "G#",
                   "A", "A#", "B", "C", "C#", "D", "D#", "E", "F", "F#", "G", "G#",
                   "A", "A#", "B", "C", "C#", "D", "D#", "E", "F", "F#", "G", "G#"];
      $(this).find(".letter").html(notes[noteOffset + globalOffset]);
    });
  });

  $(".key-type").click(function(){
    $(".key-type").removeClass("selected");
    $(this).addClass("selected");
    var keyType = $(this).html();
    $("#tool-key-type").html(keyType);
    if (keyType === "Minor") {
      $(".major-covers .minor-cover").css("display", "inline-block");
      $(".other-minor-chords").show();
      $(".other-chords").hide();
    } else {
      $(".major-covers .minor-cover").css("display", "none");
      $(".other-minor-chords").hide();
      $(".other-chords").show();
    }
  });

</script>


<script type="text/javascript">
  $(".note.active").click(function(){
    var freq = $(this).attr("attr-freq");
    playNote(freq);
  });

  $(".correct").click(function(){
    $(this).css("background-color", "#3FBC46")
           .css("color", "#FFF");
  });

  $(".incorrect").click(function(){
    $(this).css("background-color", "#222")
           .css("color", "#FFF")
           .css("text-decoration", "line-through");
  });

  $(".major").hover(function(){
    $("#count1").removeClass("major minor diminished");
    $("#count1").addClass("major");
    var offset = parseInt($(this).attr("data-offset"));
    $("#count1").css("margin-left", 5+(41*offset)+"px");
  }, function() {
    $("#count1").removeClass("major minor diminished");
  });

  $(".minor").hover(function(){
    $("#count1").removeClass("major minor diminished");
    $("#count1").addClass("minor");
    var offset = parseInt($(this).attr("data-offset"));
    $("#count1").css("margin-left", 5+(41*offset)+"px");
  }, function() {
    $("#count1").removeClass("major minor diminished");
  });

  $(".diminished").hover(function(){
    $("#count1").removeClass("major minor diminished");
    $("#count1").addClass("diminished");
    var offset = parseInt($(this).attr("data-offset"));
    $("#count1").css("margin-left", 5+(41*offset)+"px");
  }, function() {
    $("#count1").removeClass("major minor diminished");
  });
</script>

  </div>

</article>

